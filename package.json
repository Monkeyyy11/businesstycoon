    "homepage": "https://github.com/Monkeyyy11/buisnesscompany#readme",
    "dependencies": {
        "body-parser": "^1.19.0",
        "chalk": "^2.4.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.4",
        "cookie-session": "^1.3.3",
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-handlebars": "^3.1.0",
        "express-session": "^1.16.2",
        "handlebars-helpers": "^0.10.0",
        "helmet": "^3.20.0",
        "mongodb": "^4.0.0",
        "mongoose": "^5.6.9",
        "passport": "^0.4.0",
        "passport-google-oauth": "^2.0.0",
        "password": "^0.1.1",
        "path": "^0.12.7",
        "remarkable": "^1.7.2",
        "serve-favicon": "^2.5.0",
        "underscore.string": "^3.3.5"
    },
    "devDependencies": {
        "eslint": "^6.1.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.18.2"
    },
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ],
            "no-multi-assign": [
                "off"
            ],
            "guard-for-in": [
                "off"
            ]
        }
    }
}
